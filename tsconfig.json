{
  "compilerOptions": {
    "target": "esnext",
    "moduleResolution": "node",
    "baseUrl": "./",
    "jsx": "react",
    "declaration": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "paths": {
      "@/*": ["src/*"],
      "@@/*": [".dumi/tmp/*"],
      "rc-picker": ["src/index.tsx"]
    }
  },
  "include": [
    ".dumirc.ts",
    "src/**/*.ts",
    "src/**/*.tsx",
    "docs/examples/focus.tsx",
    "tests/**/*.ts",
    "tests/**/*.tsx"
  ]
}
